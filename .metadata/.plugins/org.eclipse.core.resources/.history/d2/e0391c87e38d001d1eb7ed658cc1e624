package com.dataTypes;

public class ConstructorOverloading {
	
	private String name;
	private int id;
	
	public ConstructorOverloading() {
		this.name="Default Name";
	}
	
	public ConstructorOverloading(String n) {
		this.name=n;
	}
	
	public ConstructorOverloading(String n,int id) {
		this.name=n;
		this.id=id;
	}

	
	@Override
	public String toString() {
		return "ConstructorOverloading [name=" + name + ", id=" + id + "]";
	}

	
	public static void main(String[] args) {
		
		ConstructorOverloading co=new ConstructorOverloading();
		System.out.println(co);
		
		co=new ConstructorOverloading("Debasish Pruseth");
		System.out.println(co);
		
		co=new ConstructorOverloading("Java",34);
		System.out.println(co);
		
	}

}
