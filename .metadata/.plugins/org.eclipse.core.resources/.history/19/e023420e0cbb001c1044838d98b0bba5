package com.debasish;

public class HelloWorld {

	public static void main(String[] args) {
		
		new Thread(new Runnable() {
			
			@Override
			public void run() {
				System.out.println("Thread is Started");
				
			}
		}).start();
		
		//new Thread(()->System.out.println("Thread is Started")).start();

	}

}
